<!DOCTYPE html>
<html>
    <head>
        <title>Users index page</title>
    </head>
    <body>
        <h1>Users</h1>
        <table>
            <tr>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Age</th>
                <th>Email</th>
                #<th>Password</th>
            </tr>
           
            <% User.all.each do |users| %>
            <tr>
                <td class="tr"><%=users.firstname%></td>
                <td class="tr"><%=users.lastname%></td>
                <td class="tr"><%=users.age%></td>
                <td class="tr"><%=users.email%></td>
                #<td class="tr"><%=users.password%></td>
            </tr>
            <%end %>
            
        </table>
    </body>
</html>